  "private": true,
  "scripts": {
    "dev": "yarn turbo run dev",
    "build": "yarn turbo run build",
    "test": "yarn turbo run test",
    "lint": "yarn turbo run lint",
    "clean": "yarn ts-node scripts/clean.ts",
    "clean:dry-run": "yarn ts-node scripts/clean.ts --dry-run",
    "clean:backup": "yarn ts-node scripts/clean.ts --backup",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "setup": "yarn ts-node scripts/setup.ts",
    "verify-env": "yarn ts-node scripts/verify-env.ts",
    "predev": "yarn verify-env",
    "prebuild": "yarn verify-env",
    "postinstall": "ts-node scripts/setup.ts",
    "clean:all": "yarn clean && yarn clean:electron && yarn clean:mobile",
    "clean:electron": "rimraf apps/desktop/dist apps/desktop/build .electron-builder",
    "clean:mobile": "rimraf apps/mobile/.expo apps/mobile/android/build apps/mobile/ios/build",
    "verify-env:all": "yarn verify-env && yarn turbo run verify-env",
    "setup:mobile": "cd apps/mobile && npm install --legacy-peer-deps",
    "setup:desktop": "cd apps/desktop && npm install --legacy-peer-deps && npm run electron:rebuild",
    "setup:all": "npm run setup:mobile && npm run setup:desktop",
    "validate": "yarn ts-node scripts/validate-tools.ts",
    "presetup": "yarn validate",
    "backup": "yarn ts-node scripts/backup.ts"
  },
